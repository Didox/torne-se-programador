<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsremedio" targetNamespace="http://www.tempuri.org/dsremedio.xsd" xmlns:mstns="http://www.tempuri.org/dsremedio.xsd" xmlns="http://www.tempuri.org/dsremedio.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsremedio" msdata:IsDataSet="true" msdata:Locale="pt-BR">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="remedios">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="codremedio" msdata:AutoIncrement="true" type="xs:int" />
              <xs:element name="indicacao" type="xs:string" minOccurs="0" />
              <xs:element name="preco" type="xs:decimal" minOccurs="0" />
              <xs:element name="remedio" type="xs:string" minOccurs="0" />
              <xs:element name="tipo" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:remedios" />
      <xs:field xpath="mstns:codremedio" />
    </xs:unique>
  </xs:element>
</xs:schema>